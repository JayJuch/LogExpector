<EventTypes>
    <EventType id="TaskSet">
        <RegEx>sending message: TaskSet to '(\w*)'.*TaskSet jobId="(\d)".*externalJobId="(\w*)".*podId="(\d*)".*milestoneId="(\d*)"</RegEx>
        <Fields>
            <Field id="StationId" />
            <Field id="JobId" isKey="true" />
            <Field id="ExternalJobID" isKey="true" />
            <Field id="PodId" />
            <Field id="MilestoneId" isKey="true" />
        </Fields>
    </EventType>

